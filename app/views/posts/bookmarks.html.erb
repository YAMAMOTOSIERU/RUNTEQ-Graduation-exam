<% content_for(:title, t('.title')) %>
<div class="container pt-5">
  <div class="container">
    <div class="row">
      <div class="col">
        <% if @bookmark_posts.present? %>
          <%= render @bookmark_posts %>
        <% else %>
          <p><%= t('.no_result') %></p>
        <% end %>
      </div>
      <%= paginate @bookmark_posts %>
    </div>
  </div>
</div>